		JButton btn = gd1.btnNewButton;
		if(e.getKeyCode() == e.VK_ENTER ) {
			if(e.getSource() == btn) {
				
			}
		}
	}

	@Override
	public void keyReleased(KeyEvent e) {
		// TODO Auto-generated method stub
		
	}

}
